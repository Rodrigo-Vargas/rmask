{
   "name": "rmask",
   "version": "0.1.3",
   "main": "lib/index.js",
   "scripts": {
      "build": "npm run clear && babel --out-dir lib src",
      "clear": "rimraf lib",
      "lint": "eslint src/*.js",
      "test": "mocha tests/**/*.spec.js --require @babel/register --require setup.js",
      "test:coverage": "nyc npm test"
   },
   "devDependencies": {
      "@babel/cli": "^7.15.7",
      "@babel/core": "^7.15.5",
      "@babel/preset-env": "^7.15.6",
      "@babel/register": "^7.15.3",
      "chai": "^4.3.4",
      "eslint": "^7.32.0",
      "eslint-config-airbnb": "^18.2.1",
      "eslint-plugin-import": "^2.24.2",
      "eslint-plugin-jsx-a11y": "^6.4.1",
      "eslint-plugin-react": "^7.25.2",
      "jsdom": "^17.0.0",
      "jsdom-global": "^3.0.2",
      "mocha": "^9.1.1",
      "nyc": "^15.1.0"
   },
   "files": [
      "lib"
   ],
   "nyc": {
      "reporter": [
         "text",
         "html"
      ],
      "exclude": [
         "tests/**"
      ]
   }
}
